{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="title">List of Wish</h1>

    <div class="container">
    {% for wish in wishes %}
        <a href="{{ path('whish_detail', {'id': wish.id}) }}">
        <h4>{{ wish.title }} [{{ wish.category.name }}]</h4>
        </a>
    {% endfor %}
    </div>

{% endblock %}

{% block title %}
    {{ parent() }} | List of Wish
{% endblock %}

